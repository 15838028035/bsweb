<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" 
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="upmUserGroup">

    <typeAlias alias="upmUserGroup" type="com.lj.app.bsweb.upm.user.entity.UpmUserGroup"/>

    <resultMap id="upmUserGroupResult" class="upmUserGroup">
   	  	<result property="id" column="id"/>
   	  	<result property="userGroupCode" column="user_group_code"/>
   	  	<result property="userGroupName" column="user_group_name"/>
   	  	<result property="bussinessCode" column="bussiness_Code"/>
   	  	<result property="parentId" column="parentId"/>
   	  	<result property="createBy" column="create_By"/>
   	  	<result property="createDate" column="create_Date"/>
   	  	<result property="updateBy" column="update_By"/>
   	  	<result property="updateDate" column="update_Date"/>
   	  	<result property="enableFlag" column="enable_Flag"/>
   	  	<result property="lockStatus" column="lock_Status"/>
    </resultMap>

	<sql id="dynamicWhere">
	    <dynamic prepend="WHERE">
	       <isNotEmpty prepend="AND" property="id">
	             id = #id#
	       </isNotEmpty>
	       <isNotEmpty prepend="AND" property="userGroupCode">
	             user_group_code = #userGroupCode#
	       </isNotEmpty>
	        <isNotEmpty prepend="AND" property="bussinessCode">
	             bussiness_Code = #bussinessCode#
	       </isNotEmpty>
	       <isNotEmpty prepend="AND" property="userGroupName">
	             user_group_name = #userGroupName#
	       </isNotEmpty>
	       <isNotEmpty prepend="AND" property="parentId">
	             parentId = #parentId#
	       </isNotEmpty>
	       <isNotEmpty prepend="AND" property="createBy">
	             create_By = #createBy#
	       </isNotEmpty>
	       <isNotEmpty prepend="AND" property="createDate">
	             create_Date = #createDate#
	       </isNotEmpty>
	       <isNotEmpty prepend="AND" property="updateBy">
	             update_By = #updateBy#
	       </isNotEmpty>
	       <isNotEmpty prepend="AND" property="updateDate">
	             update_Date = #updateDate#
	       </isNotEmpty>
	       <isNotEmpty prepend="AND" property="enableFlag">
	             enable_Flag = #enableFlag#
	       </isNotEmpty>
	       <isNotEmpty prepend="AND" property="lockStatus">
	             lock_Status = #lockStatus#
	       </isNotEmpty>
	    </dynamic>	
	</sql>

	<sql id="commonSelectColumnsPrefix">
	    <![CDATA[
	    select
        	id ,
        	user_group_code ,
        	bussiness_Code,
        	user_group_name ,
        	parentId ,
        	create_By ,
        	DATE_FORMAT(create_Date,'%Y-%m-%d %H:%i:%s') as create_Date ,
        	update_By ,
        	DATE_FORMAT(update_Date,'%Y-%m-%d %H:%i:%s') as update_Date ,
        	enable_Flag ,
        	lock_Status 
	    ]]>
	</sql>
	
    <insert id="insert" parameterClass="upmUserGroup">
	<selectKey resultClass="int" keyProperty="id" type="post" >
             SELECT LAST_INSERT_ID()
        </selectKey>

	    <![CDATA[
	        INSERT INTO
	        upm_user_group (
	        	id ,
	        	user_group_code ,
	        	bussiness_Code,
	        	user_group_name ,
	        	parentId ,
	        	create_By ,
	        	create_Date ,
	        	update_By ,
	        	update_Date ,
	        	enable_Flag ,
	        	lock_Status 
	        ) VALUES (
				#id# ,
				#userGroupCode# ,
				#bussinessCode#,
				#userGroupName# ,
				#parentId# ,
				#createBy# ,
				#createDate# ,
				#updateBy# ,
				#updateDate# ,
				#enableFlag# ,
				#lockStatus# 
	        )
	    ]]>
	        </insert>

    <update id="update" parameterClass="upmUserGroup">
    <![CDATA[
        UPDATE upm_user_group SET
			id = #id# ,
			user_group_code = #userGroupCode# ,
			bussiness_Code = #bussinessCode#,
			user_group_name = #userGroupName# ,
			parentId = #parentId# ,
			create_By = #createBy# ,
			create_Date = #createDate# ,
			update_By = #updateBy# ,
			update_Date = #updateDate# ,
			enable_Flag = #enableFlag# ,
			lock_Status = #lockStatus# 
        WHERE 
	        id = #id# 
    ]]>
    </update>
    
    <update id="update2" parameterClass="upmUserGroup">
    <![CDATA[
        UPDATE upm_user_group SET
			id = #id# ,
			user_group_code = #userGroupCode# ,
			bussiness_Code = #bussinessCode#,
			user_group_name = #userGroupName#,
			create_By = #createBy# ,
			create_Date = #createDate# ,
			update_By = #updateBy# ,
			update_Date = #updateDate# ,
			enable_Flag = #enableFlag# ,
			lock_Status = #lockStatus# 
        WHERE 
	        id = #id# 
    ]]>
    </update>

    <delete id="delete" parameterClass="java.lang.Integer">
    <![CDATA[
        delete from upm_user_group where
        id = #id# 
    ]]>
    </delete>
     
    <select id="select" resultMap="upmUserGroupResult">
        <include refid="commonSelectColumnsPrefix"/>
        <![CDATA[
            from upm_user_group 
        ]]>
        <include refid="dynamicWhere"/>
    </select>
    
    <select id="getInfoByKey" resultMap="upmUserGroupResult">
	    <include refid="commonSelectColumnsPrefix"/>
	    <![CDATA[
	        from upm_user_group 
	        where 
		        id = #id# 
	    ]]>
    </select>

 	<select id="pagenate" resultMap="upmUserGroupResult">
    	<include refid="commonSelectColumnsPrefix"/>
	    <![CDATA[
	        from upm_user_group 
	    ]]>
		<include refid="dynamicWhere"/>
		<dynamic prepend="ORDER BY">
			<isNotEmpty property="sortColumns">$sortColumns$</isNotEmpty>
		</dynamic>
    </select>
    
    <select id="pagenate_count" resultClass="long">
    <![CDATA[
        select 
        count(*)
        from upm_user_group
    ]]>
		<include refid="dynamicWhere"/>    
    </select>

</sqlMap>
